# CYNIC Agent Configuration
# Copy this to .env and fill in your values

# Solana RPC URL (required for real monitoring)
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com

# Agent wallet address (for trading)
AGENT_WALLET_ADDRESS=

# GASdf Relayer URL (for gasless transactions)
GASDF_RELAYER_URL=http://localhost:3000

# Dry run mode (default: true for safety)
# Set to "false" for real transactions
DRY_RUN=true

# Optional: Private key for signing (NEVER commit this!)
# AGENT_WALLET_PRIVATE_KEY=
